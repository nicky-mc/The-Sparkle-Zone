{
  "extends": ["next/babel", "next/core-web-vitals"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": ["**/*.test.js", "**/*.spec.js"] }
    ],
    "no-unused-vars": "warn",
    "no-console": "warn"
  }
}
